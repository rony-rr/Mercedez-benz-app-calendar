{
  "name": "mercedes-connect",
  "version": "2.6.02",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "updateProject": "expo-cli upgrade && expo-cli doctor && expo doctor --fix-dependencies &&  yarn upgrade --latest && yarn install",
    "publisChannel": "expo publish",
    "eas:config": "eas build:configure",
    "buildAPK": "eas build -p android --profile preview4APK",
    "buildIOS": "eas build -p ios --profile previewIOS1",
    "compiledAndroid": "eas build --platform android",
    "compiledIOS": "eas build --platform ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.7",
    "date-fns": "^2.29.3",
    "expo": "^48.0.0",
    "expo-font": "~11.1.1",
    "expo-image-picker": "~14.1.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-updates": "~0.16.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-calendars": "^1.1290.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-select-dropdown": "^2.0.4",
    "react-native-simple-time-picker": "^1.3.11",
    "react-native-web": "~0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
